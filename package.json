{
  "name": "fund-price-monitor-backend",
  "version": "0.1.0",
  "bin": {
    "fund-price-monitor-backend": "bin/fund-price-monitor-backend.js"
  },
  "scripts": {
    "build": "tsc && ./node_modules/.bin/webpack",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "local": "npm run build && cdk synth --no-staging > template.yaml",
    "invoke": "npm run local && sam local invoke",
    "deploy": "cdk deploy --profile root-programmatic-admin FundPriceMonitorBackendStack",
    "pushDeploy": "git push && npm run deploy"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.59.0",
    "@types/aws-lambda": "^8.10.61",
    "@types/jest": "^26.0.4",
    "@types/node": "10.17.27",
    "@types/puppeteer": "^3.0.1",
    "@types/superagent": "^4.1.9",
    "aws-cdk": "1.59.0",
    "aws-lambda-mock-context": "^3.2.1",
    "file-loader": "^6.0.0",
    "jest": "^26.0.4",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.1.0",
    "typescript": "~3.9.6",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@aws-cdk/aws-dynamodb": "1.59.0",
    "@aws-cdk/aws-events": "1.59.0",
    "@aws-cdk/aws-events-targets": "1.59.0",
    "@aws-cdk/aws-iam": "1.59.0",
    "@aws-cdk/aws-lambda": "1.59.0",
    "@aws-cdk/core": "1.59.0",
    "aws-sdk": "2.739.0",
    "chrome-aws-lambda": "^5.2.1",
    "puppeteer": "^5.2.1",
    "puppeteer-core": "^5.2.1",
    "source-map-support": "^0.5.16"
  }
}
