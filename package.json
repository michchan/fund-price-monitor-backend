{
  "name": "fund-price-monitor-backend",
  "version": "0.62.1",
  "bin": {
    "fund-price-monitor-backend": "bin/fund-price-monitor-backend.ts"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "construct",
        "scripts"
      ],
      "extensions": "ts",
      "ignore": "src/services/cron/handlers/__*.ts",
      "quiet": false
    },
    "build-dev": {
      "patterns": [
        "src",
        "construct",
        "scripts"
      ],
      "extensions": "ts",
      "quiet": false
    }
  },
  "scripts": {
    "analyze": "webpack-bundle-analyzer",
    "build": "npm run build-scrapers && tsc && webpack && npm run rm-scrapers-cache",
    "build-dev": "tsc && webpack",
    "build-scrapers": "ts-node scripts/buildScrapers",
    "cdk": "cdk",
    "clear-build-cache": "rm -rf ./build && rm -rf ./bundles",
    "deploy": "cdk deploy FundPriceMonitorBackendStack --require-approval never",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint-fix": "npm run eslint -- --fix",
    "invoke": "sam local invoke --profile=$AWS_PROFILE",
    "rm-scrapers-cache": "ts-node scripts/removeScrapersCache",
    "start-api": "sam local start-api --profile=$AWS_PROFILE -p $SAM_API_PORT",
    "template": "npm run build && cdk synth --no-staging > template.yaml --profile=$AWS_PROFILE",
    "test": "jest",
    "tsc-watch": "tsc --watch --noemit",
    "watch": "npm-watch build-dev",
    "watch-all": "npm-watch build"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.110.1",
    "@aws-cdk/aws-apigateway": "1.110.1",
    "@aws-cdk/aws-dynamodb": "1.110.1",
    "@aws-cdk/aws-events": "1.110.1",
    "@aws-cdk/aws-events-targets": "1.110.1",
    "@aws-cdk/aws-iam": "1.110.1",
    "@aws-cdk/aws-lambda": "1.110.1",
    "@aws-cdk/aws-logs": "1.110.1",
    "@aws-cdk/aws-logs-destinations": "1.110.1",
    "@aws-cdk/aws-s3": "1.110.1",
    "@aws-cdk/aws-sns": "1.110.1",
    "@aws-cdk/aws-sns-subscriptions": "1.110.1",
    "@aws-cdk/aws-stepfunctions": "1.110.1",
    "@aws-cdk/aws-stepfunctions-tasks": "1.110.1",
    "@aws-cdk/core": "1.110.1",
    "@michchan/eslint-config": "1.9.0",
    "@michchan/webpack-config-aws-cdk": "0.5.1",
    "@types/aws-lambda": "^8.10.70",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.167",
    "@types/node": "14.14.20",
    "@types/puppeteer": "^5.4.2",
    "aws-cdk": "1.110.1",
    "aws-lambda-mock-context": "^3.2.1",
    "aws-sdk": "2.824.0",
    "eslint": "^7.11.0",
    "file-loader": "^6.0.0",
    "jest": "^26.6.1",
    "npm-watch": "0.7.0",
    "ts-jest": "^26.4.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "webpack": "^5.12.2",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "@michchan/fund-price-monitor-lib": "0.7.0",
    "@octokit/core": "^3.4.0",
    "axios": "^0.21.1",
    "chrome-aws-lambda": "5.5.0",
    "dayjs": "^1.10.5",
    "dotenv": "^8.2.0",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "puppeteer": "5.5.0",
    "puppeteer-core": "5.5.0",
    "simply-utils": "0.57.1",
    "source-map-support": "^0.5.19"
  }
}
