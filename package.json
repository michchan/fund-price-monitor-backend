{
  "name": "fund-price-monitor-backend",
  "version": "0.24.0",
  "bin": {
    "fund-price-monitor-backend": "bin/fund-price-monitor-backend.ts"
  },
  "scripts": {
    "analyze": "webpack-bundle-analyzer",
    "build": "npm run build:scrapers && tsc && webpack",
    "build:scrapers": "node scripts/buildScrapers",
    "cdk": "cdk",
    "clearbuildcache": "rm -rf ./build && rm -rf ./bundles",
    "deploy": "cdk deploy FundPriceMonitorBackendStack --require-approval never",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "npm run eslint -- --fix",
    "invoke": "npm run template && sam local invoke",
    "template": "npm run build && cdk synth --no-staging > template.yaml",
    "test": "jest",
    "watch": "tsc --watch --noemit"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.72.0",
    "@michchan/eslint-config": "1.6.0",
    "@types/aws-lambda": "^8.10.61",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.160",
    "@types/node": "14.14.2",
    "@types/puppeteer": "^3.0.1",
    "aws-cdk": "1.72.0",
    "aws-lambda-mock-context": "^3.2.1",
    "eslint": "^7.11.0",
    "file-loader": "^6.0.0",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.72.0",
    "@aws-cdk/aws-dynamodb": "1.72.0",
    "@aws-cdk/aws-events": "1.72.0",
    "@aws-cdk/aws-events-targets": "1.72.0",
    "@aws-cdk/aws-iam": "1.72.0",
    "@aws-cdk/aws-lambda": "1.72.0",
    "@aws-cdk/aws-logs": "1.72.0",
    "@aws-cdk/aws-logs-destinations": "1.72.0",
    "@aws-cdk/aws-sns": "1.72.0",
    "@aws-cdk/aws-sns-subscriptions": "1.72.0",
    "@aws-cdk/core": "1.72.0",
    "@aws-cdk/aws-stepfunctions": "1.72.0",
    "@aws-cdk/aws-stepfunctions-tasks": "1.72.0",
    "aws-sdk": "2.777.0",
    "axios": "^0.20.0",
    "chrome-aws-lambda": "5.3.1",
    "dotenv": "^8.2.0",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.20",
    "puppeteer-core": "5.3.1",
    "puppeteer": "5.3.1",
    "simply-utils": "0.49.0",
    "source-map-support": "^0.5.19"
  }
