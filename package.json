{
  "name": "fund-price-monitor-backend",
  "version": "0.1.0",
  "bin": {
    "fund-price-monitor-backend": "bin/fund-price-monitor-backend.ts"
  },
  "scripts": {
    "buildScrapers": "node scripts/buildScrapers",
    "build": "npm run buildScrapers && tsc && ./node_modules/.bin/webpack",
    "clearBuildCache": "rm -rf ./build && rm -rf ./bundles",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "template": "npm run build && npm run cdk synth --no-staging > template.yaml",
    "invoke": "npm run template && sam local invoke",
    "deploy": "npm run cdk deploy FundPriceMonitorBackendStack -- --require-approval never"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.68.0",
    "@types/aws-lambda": "^8.10.61",
    "@types/jest": "^26.0.4",
    "@types/lodash": "^4.14.160",
    "@types/node": "10.17.27",
    "@types/puppeteer": "^3.0.1",
    "@types/superagent": "^4.1.9",
    "aws-cdk": "1.68.0",
    "aws-lambda-mock-context": "^3.2.1",
    "file-loader": "^6.0.0",
    "jest": "^26.0.4",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.68.0",
    "@aws-cdk/aws-dynamodb": "1.68.0",
    "@aws-cdk/aws-events": "1.68.0",
    "@aws-cdk/aws-events-targets": "1.68.0",
    "@aws-cdk/aws-iam": "1.68.0",
    "@aws-cdk/aws-lambda": "1.68.0",
    "@aws-cdk/aws-logs": "1.68.0",
    "@aws-cdk/aws-logs-destinations": "1.68.0",
    "@aws-cdk/aws-sns": "1.68.0",
    "@aws-cdk/aws-sns-subscriptions": "1.68.0",
    "@aws-cdk/core": "1.68.0",
    "aws-sdk": "2.739.0",
    "axios": "^0.20.0",
    "chrome-aws-lambda": "^5.2.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.20",
    "puppeteer": "^5.2.1",
    "puppeteer-core": "^5.2.1",
    "simply-utils": "0.36.0",
    "source-map-support": "^0.5.16"
  }
}
